import requests
import json

url = "http://127.0.0.1:8000/run_code"   # ðŸ‘ˆ adjust port if needed

payload = {
    "language": "python",
    "code": """
def solve(x):
    return x + 1
""",
    "stdin": "1",
    "expected_output": "2",
    "test_cases": [
        {"input": "1", "expected": "2"},
        {"input": "5", "expected": "6"}
    ]
}

resp = requests.post(url, json=payload)
print("STATUS:", resp.status_code)
print(json.dumps(resp.json(), indent=2))
